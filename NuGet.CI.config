<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="LocalCI" value="./artifacts/nupkg" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="Umbraco Prereleases" value="https://www.myget.org/F/umbracoprereleases/api/v3/index.json" />
    <add key="Umbraco Nightly" value="https://www.myget.org/F/umbraconightly/api/v3/index.json" />
  </packageSources>
  <packageSourceMapping>
    <!-- LocalCI is checked first for Umbraco.AI packages (from current build) -->
    <packageSource key="LocalCI">
      <package pattern="Umbraco.AI" />
      <package pattern="Umbraco.AI.*" />
    </packageSource>
    <!-- Fallback to nuget.org for Umbraco.AI packages (published releases) -->
    <packageSource key="nuget.org">
      <package pattern="Umbraco" />
      <package pattern="Umbraco.*" />
      <package pattern="Umbraco.AI" />
      <package pattern="Umbraco.AI.*" />
      <package pattern="*" />
    </packageSource>
    <!-- Fallback to nightly for Umbraco.AI packages (prereleases) -->
    <packageSource key="Umbraco Nightly">
      <package pattern="Umbraco" />
      <package pattern="Umbraco.*" />
      <package pattern="Umbraco.AI" />
      <package pattern="Umbraco.AI.*" />
    </packageSource>
    <packageSource key="Umbraco Prereleases">
      <package pattern="Umbraco" />
      <package pattern="Umbraco.*" />
      <package pattern="Umbraco.AI" />
      <package pattern="Umbraco.AI.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
