{
  "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/master/src/NerdBank.GitVersioning/version.schema.json",
  "version": "1.0.0",
  "assemblyVersion": {
    "precision": "build"
  },
  "gitCommitIdShortFixedLength": 7,
  "nugetPackageVersion": {
    "semVer": 2
  },
  "publicReleaseRefSpec": [
    "^refs/heads/main$",
    "^refs/heads/hotfix/",
    "^refs/heads/release/",
    "^refs/tags/release-",
    "^refs/tags/hotfix-"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": false
    }
  }
}
