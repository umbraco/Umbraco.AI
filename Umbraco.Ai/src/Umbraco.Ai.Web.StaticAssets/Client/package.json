{
  "name": "@umbraco-ai/core",
  "version": "17.0.0",
  "type": "module",
  "types": "./types/umbraco-ai-public-types.d.ts",
  "files": [
    "types/umbraco-ai-public-types.d.ts",
    "README.md"
  ],
  "scripts": {
    "watch": "tsc && vite build --watch",
    "build": "npm run build:api && npm run build:app",
    "build:app": "tsc -p tsconfig.app.json && vite build",
    "build:api": "tsc -p tsconfig.api.json && (api-extractor run -c api-extractor.json --verbose || exit 0)",
    "prepack:api": "node ./scripts/sync-package-version.js && node ./scripts/cleanse-pkg.js && npx mkdirp ./artifacts",
    "pack:api": "npm pack --pack-destination ./artifacts",
    "postpack:api": "node ./scripts/copy-package-to-output-dir.js",
    "build-and-pack:api": "npm run build:api && npm run pack:api",
    "generate-client": "node scripts/generate-openapi.js https://localhost:44355/umbraco/swagger/ai-management/swagger.json"
  },
  "peerDependencies": {
    "@umbraco-cms/backoffice": "^17.1.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.85.0",
    "@microsoft/api-extractor": "^7.55.1",
    "@types/diff": "^7.0.2",
    "@umbraco-cms/backoffice": "^17.1.0",
    "chalk": "^5.6.0",
    "cross-env": "^10.0.0",
    "node-fetch": "^3.3.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "diff": "^8.0.3"
  }
}
